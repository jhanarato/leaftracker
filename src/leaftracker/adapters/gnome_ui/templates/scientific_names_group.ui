<?xml version='1.0' encoding='UTF-8'?>
<interface>
    <template class="ScientificNamesGroup" parent="AdwPreferencesGroup">
        <property name="title">Scientific Name</property>
        <property name="header-suffix">
            <object class="GtkButton">
                <property name="label">New Current Name</property>
            </object>
        </property>
        <child>
            <object class="AdwPreferencesRow">
                <property name="title">Current</property>
                <child>
                    <object class="GtkEntry" id="taxon_name_entry"/>
                </child>
            </object>
        </child>
        <child>
            <object class="AdwExpanderRow">
                <property name="title">Previous Names</property>
                <child>
                    <object class="AdwPreferencesRow">
                        <property name="title">Previous</property>
                        <child>
                            <object class="GtkEntry" id="previous_taxon_name_entry"/>
                        </child>
                    </object>
                </child>
            </object>
        </child>
    </template>
</interface>